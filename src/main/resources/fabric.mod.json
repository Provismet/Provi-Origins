{
  "schemaVersion": 1,
  "id": "provi-origins",
  "version": "${version}",

  "name": "Provi's Origins",
  "description": "Adds a large collection of content, include origins, powers, actions, and keybinds.",
  "authors": [
    "Provismet"
  ],
  "contact": {
    "homepage": "https://github.com/Provismet/Provi-Origins",
    "sources": "https://github.com/Provismet/Provi-Origins",
    "issues": "https://github.com/Provismet/Provi-Origins/issues"
  },

  "license": "Apache-2.0",
  "icon": "assets/proviorigins/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.provismet.proviorigins.ProviOriginsMain"
    ],
    "client": [
      "com.provismet.proviorigins.ProviOriginsClient"
    ]
  },
  "mixins": [
    "proviorigins.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric-api": "*",
    "minecraft": "~1.20",
    "java": ">=17",
    "origins": ">=1.9.0"
  },

  "recommends": {
    "extrade-origins-addon": "*",
    "pehkui": "*"
  },

  "custom": {
    "modmenu": {
      "links": {
        "wiki.proviorigins.wiki": "https://github.com/Provismet/Provi-Origins/wiki"
      }
    }
  }
}
