{
    "type": "origins:multiple",

    "recharge_mana": {
        "type": "origins:action_over_time",
        "interval": 10,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "proviorigins:splinter/mana",
            "change": 1
        }
    },

    "recharge_concentration": {
        "type": "origins:action_over_time",
        "interval": 600,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "proviorigins:splinter/concentration",
            "change": 1
        }
    }
}